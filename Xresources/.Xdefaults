!URxvt.font: -*-screen-medium-*-*-*-14-*-*-*-*-*-*-*
!URxvt.boldFont: -*-screen-bold-*-*-*-14-*-*-*-*-*-*-*
URxvt.font: xft:SimSun:antialias=true:pixelsize=12:hinting=true
URxvt.boldFont: xft:SimSun:antialias=true:pixelsize=12:hinting=true

URxvt.font: -sony-*-*-*-*-*-*-*-*-*-*-*-*-*
URxvt.boldFont: -sony-*-*-*-*-*-*-*-*-*-*-*-*-*
!Xft.dpi: 96
!Xft.antialias: true
!Xft.hinting: true
!Xft.rgba: none
!Xft.autohint: true
!Xft.hintstyle: hintfull
!Xft.lcdfilter: lcddefault

Xcursor.theme: "OpenZone White Slim"
URxvt*utf8: 1
URxvt*locale: 1
URxvt.scrollBar: off
URxvt.perl-ext-common:default,url-select,keyboard-select,clipboard,autocomplete-ALL-the-things,
URxvt.urlLauncher:chromium-browser
URvxt.url-select.button: 1
URxvt.underlineURLs:true
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-c:   perl:clipboard:copy
URxvt.keysym.M-v:   perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
URxvt.clipboard.copycmd:    xsel -ib
URxvt.clipboard.pastecmd:   xsel -ob
!AMBIANCE


URxvt.depth: 32
#include "dotfiles/rice/colors/terminal/base16-xresources/base16-grayscale.light.256.xresources"
#include "dotfiles/rice/colors/terminal/base16-xresources/base16-ocean.light.256.xresources"
#include "dotfiles/rice/colors/terminal/base16-xresources/base16-atelierestuary.dark.xresources"

URxvt.background: [100]base00


